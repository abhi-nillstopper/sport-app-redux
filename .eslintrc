{
  "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "browser": true,
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
    // "localStorage": true,
    // "document": true
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "indent": 0,
    "linebreak-style": 0,
    "quotes": 0,
    "semi": 0,
    "no-empty": 0,
    "no-unused-vars": 0
  }
}
